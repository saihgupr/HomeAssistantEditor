{
  "name": "Home Assistant Editor",
  "version": "1.0.2",
  "slug": "homeassistant-editor",
  "description": "A powerful visual editor for Home Assistant automations and scripts",
  "url": "https://github.com/saihgupr/HomeAssistantEditor",
  "startup": "application",
  "boot": "auto",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "init": false,
  "hassio_api": true,
  "auth_api": true,
  "homeassistant_api": true,
  "hassio_role": "manager",
  "webui": "http://[HOST]:[PORT:54002]",
  "ingress_port": 54002,
  "ports": {
    "54002/tcp": 54002
  },
  "map": [
    "config:rw",
    "ssl"
  ],
  "environment": {
    "CONFIG_PATH": "/config"
  },
  "options": {},
  "schema": {},
  "healthcheck": {
    "test": [
      "CMD",
      "node",
      "-e",
      "fetch('http://localhost:54002/').then(r => process.exit(r.ok ? 0 : 1)).catch(() => process.exit(1))"
    ],
    "interval": "5s",
    "timeout": "3s",
    "start_period": "10s",
    "retries": 3
  },
  "codeowners": [
    "@saihgupr"
  ],
  "labels": {
    "org.opencontainers.image.title": "Home Assistant Editor",
    "org.opencontainers.image.description": "A powerful visual editor for Home Assistant automations and scripts.",
    "org.opencontainers.image.source": "https://github.com/saihgupr/HomeAssistantEditor",
    "org.opencontainers.image.licenses": "MIT"
  },
  "supported": false,
  "ingress": true,
  "panel_icon": "mdi:home-edit-outline",
  "panel_title": "Editor"
}